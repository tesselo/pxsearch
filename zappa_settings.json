{
  "staging": {
    "keep_warm": false,
    "log_level": "WARNING",
    "app_function": "pxsearch.app.main",
    "aws_region": "eu-central-1",
    "apigateway_enabled": false,
    "project_name": "pxsearch",
    "runtime": "python3.8",
    "s3_bucket": "pxsearch-staging",
    "exclude": ["alembic/", "tests/"],
    "environment_variables": {
      "POSTGRES_HOST": "search-micro.cepyvetzz6ui.eu-central-1.rds.amazonaws.com",
      "POSTGRES_DBNAME": "staging",
      "POSGRES_PORT": "5432",
      "SENTRY_DSN": "https://d8c1399a37c44cd2ae228dfb13fa800a@o640190.ingest.sentry.io/6167701"
    },
    "events": [
      {
        "function": "pxsearch.ingest.signals.ingest_usgs_signal",
        "event_source": {
          "arn": "arn:aws:sns:us-west-2:673253540267:public-c2-notify",
          "events": [
            "sns:Publish"
          ]
        }
      },
      {
        "function": "pxsearch.ingest.signals.ingest_s2_signal",
        "event_source": {
          "arn": "arn:aws:sns:us-west-2:608149789419:cirrus-v0-publish",
          "events": [
            "sns:Publish"
          ]
        }
      },
    ]
  },
  "production": {
    "extends": "staging",
    "project_name": "pxsearch",
    "s3_bucket": "pxsearch-production",
    "environment_variables": {
      "POSTGRES_DBNAME": "prod"
    }
  }
}
